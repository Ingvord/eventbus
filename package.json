{
  "name": "eventbus",
  "version": "1.0.0",
  "description": "Simple EventBus implementation for JS SPA",
  "main": "dist/eventbus.js",
  "source": "src/eventbus.js",
  "module": "dist/eventbus.js",
  "scripts": {
    "deploy": "curl -T $npm_package_main -H \"Authorization:Basic ${BINTRAY_AUTH_TOKEN}\" https://api.bintray.com/content/waltz-controls/libs/eventbus/$npm_package_version/eventbus/$npm_package_version/eventbus.js",
    "build": "microbundle src/eventbus.js -f es",
    "dev": "microbundle watch -f es",
    "test": "jest ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waltz-controls/eventbus.git"
  },
  "keywords": [
    "pubsub",
    "publisher",
    "subscriber",
    "eventbus",
    "bus",
    "event"
  ],
  "author": "Ingvord",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waltz-controls/eventbus/issues"
  },
  "homepage": "https://github.com/waltz-controls/eventbus#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "jest": "^24.9.0",
    "microbundle": "^0.11.0"
  },
  "publishConfig": {
    "registry": "https://api.bintray.com/npm/waltz-controls/npm"
  }
}
